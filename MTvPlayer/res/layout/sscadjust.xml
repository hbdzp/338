            android:id="@+id/textview_factory_ssc_lvdsmodulation"
            android:layout_width="133dip"
            android:layout_height="fill_parent"
            android:gravity="left|center_vertical"
            android:text="@string/str_factory_ssc_lvdsmodulation"
            android:textColor="#ffffff"
            android:textSize="13dip" />

        <TextView
            android:id="@+id/textview_factory_ssc_lvdsmodulation_val"
            android:layout_width="133dip"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:textColor="#ffffff"
            android:textSize="13dip" />
    </LinearLayout>
    <!-- LVDS Percentage -->

    <LinearLayout
        android:id="@+id/linearlayout_factory_ssc_lvdspercentage"
        android:layout_width="fill_parent"
